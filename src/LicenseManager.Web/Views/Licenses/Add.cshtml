<div class="row">
    <form class="col s12 m12 l12">
        <div class="row">
            <div class="input-field col s5">
                <input id="name" type="text" class="validate">
                <label for="name">Nazwa</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s2">
                <input id="count" type="text" class="validate">
                <label for="count">Ilość</label>
            </div>
            <div class="input-field col s3">
                <select data-bind="options: licenseTypes,
                                    optionsText: 'name',
                                    value: selectedLicenseType">
                                    <option>test</option>
                    </select>
                    <label>tesT</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s5">
                <input id="buyDate" type="text" class="datepicker">
                <label for="buyDate">Data zakupu</label>
            </div>
        </div>
        <div class="row">
            <div class="col s5">
                <button class="btn lighten-2 red right" data-bind="">Utwórz licencję</button>
            </div>
            
        </div>
        
    </form>
</div>  
